{
  "author": "QA Wolf",
  "name": "playwright-video",
  "license": "BSD-3-Clause-Clear",
  "version": "2.1.0",
  "description": "Capture a video of a Playwright page",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build",
    "src"
  ],
  "engines": {
    "node": ">=10.15.0"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "release": "np --no-cleanup",
    "test": "jest",
    "test:watch": "jest --watch",
    "version": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qawolf/playwright-video.git"
  },
  "bugs": {
    "url": "https://github.com/qawolf/playwright-video/issues"
  },
  "peerDependencies": {
    "playwright-core": "^1.0.2"
  },
  "dependencies": {
    "debug": "*",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^9.0.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@types/debug": "^4.1.5",
    "@types/fluent-ffmpeg": "^2.1.14",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.9",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.13.2",
    "jest": "^26.0.1",
    "np": "^6.2.3",
    "playwright": "^1.0.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "tsutils": "^3.17.1",
    "typescript": "^3.9.3"
  }
}
